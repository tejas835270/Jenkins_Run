pipeline
{
	
agent any

	environment
	{

	echo "Hello you are into pre stage"
	}

stages{


	stage('Run Build')
	{
		steps { 	
		echo $environment
		echo "Into Pre Stage"		
		}
		
	}
	stage('Run Tests')
	{
		Run $JOB_NAME = 'Test_Maven_Jenkins'
		echo "Job run"
	}

	post{
		echo "Hello Post is executed"
		}

}