pipeline
{
	
agent any


stages{


	stage('Run Build')
	{
		steps { 	
		echo $environment
		echo "Into Pre Stage"
		sh "mvn clean"		
		}
		
	}
	stage('Run Test')
	{
	steps{
		
		echo "Job run"
		sh "mvn test"
	}
	}

			stage('Run Test1')
	{
	steps{
		
		echo "Job run1"
	}
	}	
	
		stage('Run Test2')
	{
	steps{
		
		echo "Job run2"
	}
	}
}
}